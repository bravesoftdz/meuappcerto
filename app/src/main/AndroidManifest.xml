<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.regiane.meuapp.meuappcerto">

    <!-- definir permisao de usuario -->
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".NovoMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Tela1Activity" />
        <activity android:name=".Tela2Activity" />
        <activity android:name=".estabelecimento.EstabelecimentoActivity" />
        <activity android:name=".estabelecimento.EstabelecimentoListActivity" />
        <activity android:name=".avaliacao.AvaliacaoActivity" />
        <activity android:name=".avaliacao.AvaliacaoListActivity"></activity>
    </application>

</manifest>